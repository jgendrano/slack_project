<h1>Slack Messages</h1>
<ul>
<% @messages.each do |message| %>
  <li><b><%= "#{message.slack_username}"%></b> <%=
  "(#{DateTime.strptime(message.ts, '%s')}):
   #{message.slack_message}"%>

  <%= link_to "Save", pins_path(data: { text: message.slack_message,
    user: message.slack_username, ts: message.ts }), method: :post %>

 </li>
<% end %>
